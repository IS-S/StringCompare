{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "SomeProject.Web": {
      "commandName": "Project",
      "dotnetRunMessages": "true",
      "launchBrowser": true,
      "launchUrl": "http://localhost:8080/swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "HTTP_SERVER_PUBLIC_PORT": "8080",
        "HTTP_SERVER_INTERNAL_PORT": "8082",
        "SAGE_GROUP": "/",
        "SAGE_SYSTEM": "some-project",
        "SAGE_ENV": "dev",
        "DB_PASSWORD": "some-password"
      }
    }
  }
}
